#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
fi
