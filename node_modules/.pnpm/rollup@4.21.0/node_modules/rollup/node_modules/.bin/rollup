#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/dist/bin/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/dist/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/dist/bin/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/dist/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules/rollup/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/rollup@4.21.0/node_modules:/Users/jxmyh/Documents/stopsync.nosync/project/vite/掘金-基于vite的组件库工程化实战/smarty-ui-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
